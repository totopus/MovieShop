@model List<ApplicationCore.Models.FavoriteResponseModel>

<div class="rounded">
    <div class="container-fluid">
        <div class="row">
            @foreach (var movieCard in Model)
            {
                <div class="col-xl-2 col-sm-4 col-lg-3 col-6">

                    <div class="card mb-4">
                        <a asp-controller="Movies" asp-action="Details" asp-route-id="@movieCard.MovieId">
                            <img src="@movieCard.PosterUrl" class="img-fluid" alt="@movieCard.Title" />
                        </a>
                        
                      </div>
               </div>
            }
    </div>
</div>
</div>